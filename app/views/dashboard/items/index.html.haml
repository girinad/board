%h1 Listing items

%table
  %tr
    %th
    %th
    %th

  - @items.each do |item|
    %tr
      %td= link_to item.title, dashboard_item_path(item)
      %td= link_to 'Edit', edit_dashboard_item_path(item)
      %td= link_to 'X', dashboard_item_path(item), method: :delete, data: { confirm: 'Are you sure?' }

%br

= link_to 'New Item', new_dashboard_item_path
